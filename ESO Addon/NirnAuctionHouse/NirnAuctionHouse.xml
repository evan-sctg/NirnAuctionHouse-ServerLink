<GuiXml>
  <Controls>
  		<Label name="NAHRowLabel" font="ZoFontGame" wrapMode="ELLIPSIS" horizontalAlignment="LEFT" verticalAlignment="CENTER" virtual="true" />
		<Label name="NAHSearchLabel" font="ZoFontGameLargeBold" color="INTERFACE_COLOR_TYPE_TEXT_COLORS:INTERFACE_TEXT_COLOR_NORMAL" virtual="true" />
		<Control name="NAHRow" mouseEnabled="true" virtual="true">
			<Dimensions y="30" />
			<OnMouseEnter>
				NAHRow_OnMouseEnter(self)
			</OnMouseEnter>
			<OnMouseExit>
				NAHRow_OnMouseExit(self)
			</OnMouseExit>
			<OnMouseUp>
				NAHRow_OnMouseUp(self, button, upInside)
			</OnMouseUp>
			<Controls>
				<Texture name="$(parent)BG" inherits="ZO_ThinListBgStrip" />
				
				<Label name="$(parent)Name" inherits="NAHRowLabel">
					<Anchor point="LEFT"/>
					<Dimensions x="200" y="30" />
				</Label>
				<Label name="$(parent)Qty" inherits="NAHRowLabel">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Name" />
					<Dimensions x="50" y="30" />
				</Label>
				<Label name="$(parent)TimeLeft" inherits="NAHRowLabel">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Qty" />
					<Dimensions x="135" y="30" />
				</Label>
				<Label name="$(parent)Type" inherits="NAHRowLabel">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)TimeLeft" />
					<Dimensions x="125" y="30" />
				</Label>
				<Label name="$(parent)Bid" inherits="NAHRowLabel">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Type" />
					<Dimensions x="80" y="30" />
				</Label>
				<Label name="$(parent)Buyout" inherits="NAHRowLabel">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Bid" />
					<Dimensions x="80" y="30" />
				</Label>
				
		<Button name="$(parent)DoBid" inherits="ZO_DefaultButton" text="Bid" hidden="false">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Buyout" />
					<Dimensions x="100" y="30" />
					<OnInitialized>
						self:SetText(GetString(SI_NAH_BID))
					</OnInitialized>
			<OnClicked>
				NirnAuctionHouse_BidItem(self:GetParent())
			</OnClicked>
		</Button>
		<Button name="$(parent)DoBuyout" inherits="ZO_DefaultButton" text="Buyout" hidden="false">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)DoBid" />
					<Dimensions x="100" y="30" />
					<OnInitialized>
						self:SetText(GetString(SI_NAH_BUYOUT))
					</OnInitialized>
			<OnClicked>
				NirnAuctionHouse_BuyItem(self:GetParent())
			</OnClicked>
		</Button>
		<Button name="$(parent)DoCancel" inherits="ZO_DefaultButton" text="Cancel" hidden="true">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)DoBid" />
					<Dimensions x="100" y="30" />
					<OnInitialized>
						self:SetText(GetString(SI_NAH_CANCEL))
					</OnInitialized>
			<OnClicked>
				NirnAuctionHouse_CancelListing(self:GetParent())
			</OnClicked>
		</Button>
			</Controls>
		</Control>
		
		
		
		<Control name="NAHOrderRow" mouseEnabled="true" virtual="true">
			<Dimensions y="30" />
			<OnMouseEnter>
				NAHRow_OnMouseEnter(self)
			</OnMouseEnter>
			<OnMouseExit>
				NAHRow_OnMouseExit(self)
			</OnMouseExit>
			<OnMouseUp>
				NAHRow_OnMouseUp(self, button, upInside)
			</OnMouseUp>
			<Controls>
				<Texture name="$(parent)BG" inherits="ZO_ThinListBgStrip" />
				<Label name="$(parent)Price" inherits="NAHRowLabel">
					<Anchor point="LEFT" />
					<Dimensions x="80" y="30" />
				</Label>
				<Label name="$(parent)Name" inherits="NAHRowLabel">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Price" />
					<Dimensions x="200" y="30" />
				</Label>
				<Label name="$(parent)Qty" inherits="NAHRowLabel">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Name" />
					<Dimensions x="75" y="30" />
				</Label>				
				<Label name="$(parent)TimeLeft" inherits="NAHRowLabel">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Qty" />
					<Dimensions x="155" y="30" />
				</Label>
				<Label name="$(parent)IsBuyout" inherits="NAHRowLabel">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)TimeLeft" />
					<Dimensions x="150" y="30" />
				</Label>
		<Button name="$(parent)Confirm" inherits="ZO_DefaultButton" text="Confirm" hidden="false">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)IsBuyout" />
					<Anchor point="RIGHT" relativePoint="RIGHT" relativeTo="$(parent)" offsetX="-16" />
					<Dimensions x="150" y="30" />
					<OnInitialized>
						self:SetText(GetString(SI_NAH_CONFIRM))
					</OnInitialized>
			<OnClicked>
				NirnAuctionHouse_FillOrder(self:GetParent())
			</OnClicked>
		</Button>
			</Controls>
		</Control>
  
    <TopLevelControl name="NirnAuctionHousePanel" inherits="ZO_RightPanelFootPrint">
      <Controls>
				<Texture name="$(parent)BG" inherits="ZO_InsetTexture" textureFile="esoui/art/chatwindow/chat_bg_center.dds"  />
				<Button name="$(parent)AuctionHouseRefresh" inherits="ZO_DefaultButton" text="Sync" hidden="false">
					<OnInitialized>
						self:SetText(GetString(SI_NAH_SYNC))
					</OnInitialized>
				
					<Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" offsetX="-78" offsetY="54" />
					<Dimensions x="150" y="30" />
			<OnClicked>
				NirnAuctionHouse_RefreshAH()
			</OnClicked>
		</Button>
		
				<Button name="$(parent)AuctionHouse_LastPage" inherits="ZO_DefaultButton" text="Last Page" hidden="false">
				
					<OnInitialized>
						self:SetText(GetString(SI_NAH_LAST_PAGE))
					</OnInitialized>
					<Anchor point="BOTTOM" relativePoint="BOTTOM" offsetX="-78" offsetY="-10" />
					<Dimensions x="150" y="30" />
			<OnClicked>
