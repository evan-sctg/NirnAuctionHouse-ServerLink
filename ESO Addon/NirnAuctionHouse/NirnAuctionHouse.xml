					<OnInitialized>
						self:SetText(GetString(SI_NAH_PRICERANGE))
					</OnInitialized>
				</Label>
				
				<Label name="$(parent)PriceRangeDash" inherits="NAHRowLabel" text="-">
					<Anchor point="LEFT" relativePoint="LEFT" relativeTo="$(parent)PriceMin" offsetX="77" />
