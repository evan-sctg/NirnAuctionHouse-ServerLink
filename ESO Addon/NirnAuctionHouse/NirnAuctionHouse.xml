				</Control>
		
			<Control name="$(parent)FilterDropCrafting" inherits="ZO_ComboBox"  hidden="true">
					<Dimensions x="175" /><Anchor  point="LEFT" relativeTo="$(parent)FilterDropSub" relativePoint="LEFT" offsetX="0" offsetY="40" />
				</Control>
		
			<Control name="$(parent)FilterDropSlot" inherits="ZO_ComboBox">
					<Dimensions x="175" /><Anchor  point="LEFT" relativeTo="$(parent)FilterDropSub" relativePoint="LEFT" offsetX="0" offsetY="40" />
				</Control>
		
			<Control name="$(parent)FilterDropEnch" inherits="ZO_ComboBox">
