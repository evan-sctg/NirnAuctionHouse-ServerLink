							<Anchor point="TOPLEFT" relativePoint="TOPRIGHT" relativeTo="$(parent)TimeLeft" />
							<Dimensions x="125" y="32" />
						</Control>
						<Control name="$(parent)Bid" inherits="ZO_SortHeader">
