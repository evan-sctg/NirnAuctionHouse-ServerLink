						<EditBox name="$(parent)Box" inherits="ZO_DefaultEditForBackdrop ZO_EditDefaultText" />
					</Controls>
				</Backdrop>
				
				<Label name="$(parent)LevelRange" inherits="NAHRowLabel" text="Level Range" hidden="true">
					<Anchor point="LEFT" relativePoint="LEFT" relativeTo="$(parent)LevelMin" offsetY="-30" />
					<Dimensions x="150" y="30" />
					<OnInitialized>
						self:SetText(GetString(SI_NAH_LEVELRANGE))
					</OnInitialized>
				</Label>
				
				<Label name="$(parent)LevelRangeDash" inherits="NAHRowLabel" text="-">
					<Anchor point="LEFT" relativePoint="LEFT" relativeTo="$(parent)LevelMin" offsetX="77" />
					<Dimensions x="150" y="30" />
				</Label>
				
				<Control name="$(parent)LevelRangeType" resizeToFitDescendents="true" mouseEnabled="true">
					<Anchor point="LEFT" relativePoint="LEFT" relativeTo="$(parent)LevelMin" offsetY="-30" />
					<OnInitialized>
						ZO_ComboBox:New(self)
					</OnInitialized>
					<OnMouseUp>
						if (button == MOUSE_BUTTON_INDEX_LEFT and upInside) then
							ZO_ComboBox_DropdownClicked(self)
						end
					</OnMouseUp>
					<Controls>
						<Label name="$(parent)Anchor" inherits="NAHSearchLabel">
							<Anchor point="RIGHT" />
