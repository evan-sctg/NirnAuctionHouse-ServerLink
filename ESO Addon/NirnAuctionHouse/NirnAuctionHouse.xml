						<Button name="$(parent)OpenDropdown" inherits="ZO_DropdownButton">
							<Dimensions x="16" y="16" />
							<Anchor point="RIGHT" relativeTo="$(parent)SelectedItemText" relativePoint="LEFT" offsetX="-3" />
							<OnClicked>
								ZO_ComboBox_DropdownClicked(self:GetParent())
							</OnClicked>
						</Button>
					</Controls>
				</Control>
				

			
				<Control name="$(parent)Headers">
					<Anchor point="TOPLEFT" offsetX="30" offsetY="90" />
					<Anchor point="TOPRIGHT" offsetY="90" />
					<Dimensions y="32" />
					<Controls>
					
						
						<Control name="$(parent)Name" inherits="ZO_SortHeader">
							<OnInitialized>
							
								ZO_SortHeader_Initialize(self, GetString(SI_NAH_HEADER_NAME), "name", ZO_SORT_ORDER_UP, TEXT_ALIGN_LEFT, "ZoFontGameLargeBold")
								
							</OnInitialized>
							<Anchor point="TOPLEFT" offsetX="300" />
